# Ignore bundler config
/.bundle

# Ignore the default SQLite database.
/db/*.sqlite3

# Ignore all logfiles and tempfiles.
/log/*.log
/tmp
.env

#ignore the cloudinary api file
/config/cloudinary.yml

# ignore OS X DS_Store file
.DS_Store

# ignore diff orig files
*.orig

# ignore cached cloudinary files
public/uploads/tmp
